{
  "name": "object-storage",
  "version": "1.0.0-rc.1",
  "description": "Object storage using Minio/S3's AWS service",
  "main": "bin/index.js",
  "bin": {
    "object-storage": "index.js"
  },
  "scripts": {
    "object-storage": "node bin/index.js",
    "pkg": "cross-env NODE_ENV=production npm run nexe",
    "nexe": "cross-env NEXE_PLATFORM=${NEXE_PLATFORM} NEXE_ARCH=${NEXE_ARCH} NEXE_VERSION=${NEXE_VERSION} node build/nexe/nexe.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diazharizky/object-storage.git"
  },
  "keywords": [
    "node",
    "minio",
    "s3"
  ],
  "author": "Diaz Harizky Firdaus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diazharizky/object-storage/issues"
  },
  "homepage": "https://github.com/diazharizky/object-storage#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nexe": "^3.3.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "js-yaml": "^3.13.1",
    "minio": "^7.0.16",
    "pino": "^6.2.1",
    "swagger-ui-express": "^4.1.4"
  }
}
